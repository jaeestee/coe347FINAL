    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary22to16
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.050252814 1.027414343 1.027414343);
    }
    procBoundary22to21
    {
        type            processor;
        value           nonuniform List<scalar> 2(0.9745627527 0.966825194);
