    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary22to16
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.05022098 1.027911036 1.027911036);
    }
    procBoundary22to21
    {
        type            processor;
