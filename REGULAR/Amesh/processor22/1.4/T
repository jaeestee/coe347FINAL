{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary22to16
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.049840794 1.031658712 1.031658712);
    }
    procBoundary22to21
    {
        type            processor;
        value           nonuniform List<scalar> 2(0.975779701 0.9674679107);
