{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            zeroGradient;
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary29to22
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.058818251 1.056103647 1.04886344);
    }
    procBoundary29to23
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.025476164 1.009421702);
