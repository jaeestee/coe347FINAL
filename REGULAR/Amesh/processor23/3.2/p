FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "3.2";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   nonuniform List<scalar> 5(1.010298656 1.027700974 1.010108763 1.025485342 1.009400715);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            zeroGradient;
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary23to16
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.052829669 1.061650723);
    }
    procBoundary23to17
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.010745943 1.027278172 1.027278172);
    }
    procBoundary23to29
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.025246898 1.009444411);
