    outlet
    {
        type            inletOutlet;
        inletValue      uniform 1;
        value           nonuniform List<scalar> 3(1.028835311 1.031284937 1.031929066);
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary23to16
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.046418831 1.050093524);
    }
    procBoundary23to17
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.02537565 1.035266402 1.035266402);
