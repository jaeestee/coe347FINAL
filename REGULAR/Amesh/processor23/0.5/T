FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.5";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1.02422899 1.03480811 1.025985842 1.035409599 1.026469859);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 1;
        value           nonuniform List<scalar> 3(1.024233852 1.0259857 1.026417495);
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary23to16
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.04323751 1.047140031);
    }
    procBoundary23to17
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.021585475 1.03211762 1.03211762);
    }
    procBoundary23to29
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.035386909 1.026443992);
    }
}


