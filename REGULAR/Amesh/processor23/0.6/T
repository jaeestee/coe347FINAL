dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1.024702914 1.035593943 1.026919179 1.036012023 1.027517188);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 1;
        value           nonuniform List<scalar> 3(1.02469833 1.02691182 1.027471804);
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary23to16
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.044716276 1.048489334);
    }
    procBoundary23to17
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.021549238 1.032907655 1.032907655);
    }
    procBoundary23to29
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.036001462 1.027500208);
    }
