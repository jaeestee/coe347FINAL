    outlet
    {
        type            inletOutlet;
        inletValue      uniform 1;
        value           nonuniform List<scalar> 3(1.02895168 1.031378938 1.032179298);
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary23to16
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.045874149 1.050179864);
    }
    procBoundary23to17
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.025570947 1.036076231 1.036076231);
    }
    procBoundary23to29
