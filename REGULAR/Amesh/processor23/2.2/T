    {
        type            inletOutlet;
        inletValue      uniform 1;
        value           nonuniform List<scalar> 3(1.028808181 1.031288776 1.031914873);
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary23to16
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.04631685 1.050068473);
    }
    procBoundary23to17
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.025323218 1.035356605 1.035356605);
