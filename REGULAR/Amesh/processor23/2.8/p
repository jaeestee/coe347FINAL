    outlet
    {
        type            zeroGradient;
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary23to16
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.052881806 1.061629664);
    }
    procBoundary23to17
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.010685551 1.027302915 1.027302915);
