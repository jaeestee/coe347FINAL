    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary23to16
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.046383562 1.050045768);
    }
    procBoundary23to17
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.02537924 1.035340704 1.035340704);
