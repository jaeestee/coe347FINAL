FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "3.1";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   nonuniform List<scalar> 5(1 1 1 1 1);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 1;
    }
    outlet
    {
        type            zeroGradient;
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary12to6
    {
        type            processor;
        value           nonuniform List<scalar> 4(1 1 1 1);
    }
    procBoundary12to13
    {
        type            processor;
        value           nonuniform List<scalar> 2(1 1);
    }
    procBoundary12to18
    {
        type            processor;
        value           nonuniform List<scalar> 3(1 1 1);
    }
}


