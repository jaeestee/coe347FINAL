FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.2";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 8(1.000661146 1.001847495 1.000000339 1.00000875 1.0000081 0.9999999987 1.000000028 1.00000002);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary32to26
    {
        type            processor;
        value           nonuniform List<scalar> 4(1.01382928 1.026889072 1.000236222 1.000236222);
    }
    procBoundary32to31
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.000000018 0.9999999993);
    }
    procBoundary32to33
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.009147686 1.001329381 1.000050371);
    }
}


