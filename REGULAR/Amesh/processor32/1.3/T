FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1.3";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 8(1.000683814 1.001686448 1.000000372 1.000008256 1.00000909 0.999999995 0.9999999917 1.000000034);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary32to26
    {
        type            processor;
        value           nonuniform List<scalar> 4(1.013836088 1.027195603 1.000232137 1.000232137);
    }
    procBoundary32to31
    {
        type            processor;
        value           nonuniform List<scalar> 2(0.9999999777 0.9999999956);
    }
    procBoundary32to33
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.017149565 1.004243623 1.000426642);
    }
}


