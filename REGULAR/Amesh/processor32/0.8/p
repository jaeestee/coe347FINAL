FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.8";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   nonuniform List<scalar> 8(1.002337533 1.00574339 1.000001259 1.000028663 1.000031987 0.9999999828 0.999999976 1.000000124);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            zeroGradient;
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary32to26
    {
        type            processor;
        value           nonuniform List<scalar> 4(1.04769351 1.095636837 1.000793293 1.000793293);
    }
    procBoundary32to31
    {
        type            processor;
        value           nonuniform List<scalar> 2(0.9999999229 0.9999999828);
    }
    procBoundary32to33
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.059413187 1.015065005 1.001536511);
    }
}


