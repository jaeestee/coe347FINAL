FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.8";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 8(1.00068399 1.001685832 1.00000035 1.00000857 1.000009112 0.9999999951 0.9999999932 1.000000036);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary32to26
    {
        type            processor;
        value           nonuniform List<scalar> 4(1.013836538 1.027178611 1.000231875 1.000231875);
    }
    procBoundary32to31
    {
        type            processor;
        value           nonuniform List<scalar> 2(0.999999978 0.9999999951);
    }
    procBoundary32to33
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.017131047 1.004277767 1.000435164);
    }
}


