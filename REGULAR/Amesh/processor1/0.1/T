FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.1";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1 1 1 0.9999999998 1.000000001);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 1;
    }
    procBoundary1to2
    {
        type            processor;
        value           uniform 0.9999999997;
    }
    procBoundary1to6
    {
        type            processor;
        value           uniform 1;
    }
    procBoundary1to7
    {
        type            processor;
        value           nonuniform List<scalar> 5(1 1 1 1 1.000005886);
    }
    procBoundary1to8
    {
        type            processor;
        value           uniform 1.000000007;
    }
}


