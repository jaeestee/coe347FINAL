    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary16to10
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.005297123 1.041519767);
    }
    procBoundary16to21
    {
        type            processor;
        value           nonuniform List<scalar> 3(0.9745519935 0.9713705554 0.9708206937);
    }
    procBoundary16to22
