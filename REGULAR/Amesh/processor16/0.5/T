        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary16to10
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.00190465 1.038730416);
    }
    procBoundary16to21
    {
        type            processor;
        value           nonuniform List<scalar> 3(0.9759019856 0.9737306056 0.9687255166);
