dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1.016639231 1.046118968 1.031160679 1.050180694 1.02795461);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary16to10
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.004351615 1.041008992);
    }
    procBoundary16to21
    {
        type            processor;
        value           nonuniform List<scalar> 3(0.9756484023 0.970968929 0.9709324097);
    }
    procBoundary16to22
    {
        type            processor;
