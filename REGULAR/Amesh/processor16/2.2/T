FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "2.2";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1.0143232 1.04631685 1.027965622 1.050068473 1.031698371);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary16to10
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.005090228 1.041495411);
    }
    procBoundary16to21
    {
        type            processor;
        value           nonuniform List<scalar> 3(0.9745634693 0.9712517981 0.9709747153);
    }
    procBoundary16to22
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.049086251 1.049086251 1.017179886);
    }
    procBoundary16to23
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.038141758 1.038460916);
    }
