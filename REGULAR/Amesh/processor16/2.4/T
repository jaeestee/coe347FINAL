dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1.017101994 1.04619924 1.031745375 1.05022098 1.027911036);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary16to10
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.004361818 1.041041775);
    }
    procBoundary16to21
    {
        type            processor;
        value           nonuniform List<scalar> 3(0.9757851723 0.9709338087 0.9712847652);
    }
    procBoundary16to22
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.04909754 1.04909754 1.014353893);
    }
    procBoundary16to23
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.038121918 1.038438771);
    }
