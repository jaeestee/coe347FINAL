boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary16to10
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.005332798 1.041524665);
    }
    procBoundary16to21
    {
        type            processor;
        value           nonuniform List<scalar> 3(0.9745617261 0.9713973605 0.9707901635);
    }
    procBoundary16to22
    {
        type            processor;
