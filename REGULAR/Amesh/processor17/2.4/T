{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "2.4";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1.028234788 1.022844367 1.032216551 1.025422569 1.03524119);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 1;
        value           nonuniform List<scalar> 2(1.022840624 1.025408608);
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary17to10
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.02480316 1.032715939 1.041041775);
    }
    procBoundary17to11
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.006317219 1.012709821);
    }
    procBoundary17to23
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.028878591 1.028878591 1.038121918);
    }
