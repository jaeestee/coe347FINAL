FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1.8";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1.028058811 1.022811609 1.032143696 1.025303401 1.035328136);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 1;
        value           nonuniform List<scalar> 2(1.022814096 1.025291397);
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary17to10
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.024815507 1.032957417 1.041462998);
    }
    procBoundary17to11
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.006269092 1.01269869);
    }
    procBoundary17to23
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.028759191 1.028759191 1.038101908);
    }
}


