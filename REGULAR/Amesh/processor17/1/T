{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 1;
        value           nonuniform List<scalar> 2(1.022642396 1.025115635);
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary17to10
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.023814665 1.032496512 1.04129752);
    }
    procBoundary17to11
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.006695243 1.012484639);
