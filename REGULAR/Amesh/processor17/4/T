FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "4";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1.028229427 1.022849562 1.032235801 1.025421158 1.035244679);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 1;
        value           nonuniform List<scalar> 2(1.022845526 1.025415517);
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary17to10
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.024865164 1.032752448 1.040987124);
    }
    procBoundary17to11
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.006315607 1.012712592);
    }
    procBoundary17to23
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.028881554 1.028881554 1.038111395);
    }
}


