FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "4.4";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1.028227769 1.022849314 1.032240524 1.02541891 1.035247653);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 1;
        value           nonuniform List<scalar> 2(1.022845294 1.025413161);
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary17to10
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.024876484 1.032760683 1.040978235);
    }
    procBoundary17to11
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.006315406 1.012712754);
    }
    procBoundary17to23
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.02887909 1.02887909 1.038110043);
    }
}


