FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.3";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1.023626279 1.011036758 1.026023212 1.012242472 1.028518075);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 1;
        value           nonuniform List<scalar> 2(1.010930097 1.012121875);
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary17to10
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.01829713 1.026731619 1.033210594);
    }
    procBoundary17to11
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.00330684 1.004721052);
    }
    procBoundary17to23
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.013797632 1.013797632 1.030101503);
    }
