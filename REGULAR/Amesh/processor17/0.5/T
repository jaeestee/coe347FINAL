FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.5";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1.024874849 1.019394062 1.029055447 1.021585475 1.03211762);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 1;
        value           nonuniform List<scalar> 2(1.019404892 1.021598082);
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary17to10
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.022727183 1.030636704 1.038730416);
    }
    procBoundary17to11
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.001387965 1.008940094);
    }
    procBoundary17to23
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.02422899 1.02422899 1.03480811);
    }
}


