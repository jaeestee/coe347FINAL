dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1 1 1 1.000007464 1.001072435);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary7to1
    {
        type            processor;
        value           nonuniform List<scalar> 5(1 1 1 1.000000014 1.000000014);
    }
    procBoundary7to6
    {
        type            processor;
        value           uniform 1;
    }
    procBoundary7to13
    {
        type            processor;
        value           nonuniform List<scalar> 4(1 1 1 1.015769857);
