FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.7";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1 1 1 1.00000742 1.001072274);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary7to1
    {
        type            processor;
        value           nonuniform List<scalar> 5(1 1 1 0.9999999908 0.9999999908);
    }
    procBoundary7to6
    {
        type            processor;
        value           uniform 1;
    }
    procBoundary7to13
    {
        type            processor;
        value           nonuniform List<scalar> 4(1 1 1 1.01577012);
    }
    procBoundary7to14
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.00004634 1.003409594);
    }
}


