{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            zeroGradient;
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary7to1
    {
        type            processor;
        value           nonuniform List<scalar> 5(1 1 1 1.000000047 1.000000047);
    }
    procBoundary7to6
    {
        type            processor;
        value           uniform 1;
    }
    procBoundary7to13
    {
        type            processor;
        value           nonuniform List<scalar> 4(1 1 1 1.055745727);
    }
    procBoundary7to14
