FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.1";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 8(1.000005886 1.000044277 1.000000001 1.000000007 0.9999999998 0.9999999997 1 1);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary31to25
    {
        type            processor;
        value           nonuniform List<scalar> 4(1.001016393 1.003437372 1 1);
    }
    procBoundary31to26
    {
        type            processor;
        value           uniform 1.000189587;
    }
    procBoundary31to30
    {
        type            processor;
        value           nonuniform List<scalar> 2(1 1);
    }
    procBoundary31to32
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.000000231 0.9999999995);
    }
}


