{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            zeroGradient;
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary31to25
    {
        type            processor;
        value           nonuniform List<scalar> 4(1.003679935 1.011683997 1 1);
    }
    procBoundary31to26
    {
        type            processor;
        value           uniform 1.000792541;
    }
    procBoundary31to30
    {
        type            processor;
        value           nonuniform List<scalar> 2(1 1);
    }
    procBoundary31to32
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.000001264 0.9999999799);
    }
