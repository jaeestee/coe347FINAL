FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.7";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   nonuniform List<scalar> 8(1.000025662 1.000159288 0.9999999696 1.000000025 0.9999999879 0.9999999838 1 1);

