FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "3.1";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   nonuniform List<scalar> 8(1.000025816 1.000158982 1.000000047 0.9999999236 1 0.9999999847 1 1);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            zeroGradient;
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary31to25
    {
        type            processor;
        value           nonuniform List<scalar> 4(1.00368048 1.011685068 1 1);
    }
    procBoundary31to26
    {
        type            processor;
        value           uniform 1.000794101;
    }
    procBoundary31to30
    {
        type            processor;
        value           nonuniform List<scalar> 2(1 1);
    }
    procBoundary31to32
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.000001329 0.9999999831);
    }
}


