FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.4";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   nonuniform List<scalar> 8(1.000025646 1.000158663 1.000000023 1.000000057 0.9999999999 1 1 1);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            zeroGradient;
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary31to25
    {
        type            processor;
        value           nonuniform List<scalar> 4(1.003676155 1.011658405 1 1);
    }
    procBoundary31to26
    {
        type            processor;
        value           uniform 1.000795741;
    }
    procBoundary31to30
    {
        type            processor;
        value           nonuniform List<scalar> 2(1 1);
    }
    procBoundary31to32
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.000001099 0.9999999973);
    }
