FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.3";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 8(1.000007701 1.000045932 1.000000012 1.000000002 0.9999999999 1.000000002 1 1);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary31to25
    {
        type            processor;
        value           nonuniform List<scalar> 4(1.001084694 1.003410125 1 1);
    }
    procBoundary31to26
    {
        type            processor;
        value           uniform 1.000228926;
    }
    procBoundary31to30
    {
        type            processor;
        value           nonuniform List<scalar> 2(1 1);
    }
    procBoundary31to32
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.000000284 1.000000001);
    }
}


