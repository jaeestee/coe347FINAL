FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "4.4";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1.024876484 0.9894972216 1.032760683 1.004098712 1.040978235);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary10to4
    {
        type            processor;
        value           uniform 0.9991997193;
    }
    procBoundary10to15
    {
        type            processor;
        value           nonuniform List<scalar> 4(0.9776217924 0.9776217924 0.9585557484 0.9676081788);
    }
    procBoundary10to16
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.017081207 1.04614454);
    }
    procBoundary10to17
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.028227769 1.032240524 1.035247653);
    }
}


