dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1.024573281 0.9899626019 1.032719721 1.004351615 1.041008992);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary10to4
    {
        type            processor;
        value           uniform 0.9992437257;
    }
    procBoundary10to15
    {
        type            processor;
        value           nonuniform List<scalar> 4(0.9770719667 0.9770719667 0.9583759023 0.9673285736);
    }
    procBoundary10to16
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.016639231 1.046118968);
