FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1.3";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1.02413484 0.9911042424 1.032466877 1.005272084 1.041114734);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary10to4
    {
        type            processor;
        value           uniform 0.9990661004;
    }
    procBoundary10to15
    {
        type            processor;
        value           nonuniform List<scalar> 4(0.9775495378 0.9775495378 0.9587080036 0.9668481212);
    }
    procBoundary10to16
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.016417328 1.046128269);
