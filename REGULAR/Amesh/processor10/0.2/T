FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.2";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1.010429393 0.9851665123 1.017081783 0.9960019903 1.021179188);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary10to4
    {
        type            processor;
        value           uniform 0.9872009193;
    }
    procBoundary10to15
    {
        type            processor;
        value           nonuniform List<scalar> 4(0.9707809304 0.9707809304 0.954754887 0.9610441101);
    }
    procBoundary10to16
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.003702582 1.022190502);
    }
    procBoundary10to17
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.00917984 1.011394211 1.013778508);
    }
}


