dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1.024778498 0.9913939332 1.032934352 1.005297123 1.041519767);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary10to4
    {
        type            processor;
        value           uniform 0.9991825186;
    }
    procBoundary10to15
    {
        type            processor;
        value           nonuniform List<scalar> 4(0.978080236 0.978080236 0.9590588024 0.9668818573);
    }
    procBoundary10to16
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.014684716 1.0463731);
