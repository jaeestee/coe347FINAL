FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "4.1";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1.024511575 0.9909418887 1.032805496 1.0051496 1.041364481);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary10to4
    {
        type            processor;
        value           uniform 0.9991981785;
    }
    procBoundary10to15
    {
        type            processor;
        value           nonuniform List<scalar> 4(0.9774598486 0.9774598486 0.958598981 0.9669748685);
    }
    procBoundary10to16
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.016608888 1.046391658);
    }
    procBoundary10to17
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.028182422 1.032105136 1.035245226);
    }
