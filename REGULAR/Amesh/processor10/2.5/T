FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "2.5";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1.024521896 0.9910852413 1.032820885 1.005262259 1.041416255);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary10to4
    {
        type            processor;
        value           uniform 0.9991965633;
    }
    procBoundary10to15
    {
        type            processor;
        value           nonuniform List<scalar> 4(0.9775445554 0.9775445554 0.9586675308 0.9668983779);
    }
    procBoundary10to16
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.016524632 1.046418831);
    }
    procBoundary10to17
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.02816165 1.03210403 1.035266402);
    }
}


