FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "3.1";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(0.9999999956 0.9999999952 0.9999999921 1.000008289 1.000009107);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary2to1
    {
        type            processor;
        value           uniform 1;
    }
    procBoundary2to3
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.000000035 1.000000035 1.004245619);
    }
    procBoundary2to8
    {
        type            processor;
        value           nonuniform List<scalar> 5(0.9999999781 1.00000037 1.00000037 1.000683822 1.001686536);
    }
}


