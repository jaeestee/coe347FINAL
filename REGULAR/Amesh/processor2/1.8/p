FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1.8";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   nonuniform List<scalar> 5(0.9999999847 0.9999999831 0.9999999724 1.000027753 1.000031999);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            zeroGradient;
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary2to1
    {
        type            processor;
        value           uniform 1;
    }
    procBoundary2to3
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.00000012 1.00000012 1.01496624);
    }
    procBoundary2to8
    {
        type            processor;
        value           nonuniform List<scalar> 5(0.9999999237 1.000001328 1.000001328 1.002337084 1.005744832);
    }
}


