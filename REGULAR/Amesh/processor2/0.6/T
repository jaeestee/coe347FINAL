FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.6";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(0.9999999951 0.9999999941 1.000000005 1.000010073 1.000010014);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary2to1
    {
        type            processor;
        value           uniform 0.9999999956;
    }
    procBoundary2to3
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.00000006 1.00000006 1.00425004);
    }
    procBoundary2to8
    {
        type            processor;
        value           nonuniform List<scalar> 5(1.000000038 1.000000305 1.000000305 1.000683765 1.001688049);
    }
}


