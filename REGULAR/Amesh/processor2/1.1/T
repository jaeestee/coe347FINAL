FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1.1";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(0.9999999951 0.9999999951 0.9999999913 1.00000811 1.000009069);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary2to1
    {
        type            processor;
        value           uniform 1;
    }
    procBoundary2to3
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.000000033 1.000000033 1.004257385);
    }
    procBoundary2to8
    {
        type            processor;
        value           nonuniform List<scalar> 5(0.9999999765 1.00000038 1.00000038 1.000683766 1.001686254);
    }
}


