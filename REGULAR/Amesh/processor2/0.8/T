FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.8";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(0.9999999951 0.999999995 0.9999999931 1.00000857 1.000009111);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary2to1
    {
        type            processor;
        value           uniform 1;
    }
    procBoundary2to3
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.000000036 1.000000036 1.004277366);
    }
    procBoundary2to8
    {
        type            processor;
        value           nonuniform List<scalar> 5(0.999999978 1.00000035 1.00000035 1.00068399 1.001685832);
    }
}


