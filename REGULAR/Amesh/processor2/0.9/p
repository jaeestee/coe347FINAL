FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.9";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   nonuniform List<scalar> 5(0.9999999876 0.9999999834 0.9999999715 1.000027678 1.000032425);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            zeroGradient;
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary2to1
    {
        type            processor;
        value           uniform 1;
    }
    procBoundary2to3
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.000000126 1.000000126 1.015105422);
    }
    procBoundary2to8
    {
        type            processor;
        value           nonuniform List<scalar> 5(0.99999993 1.000001319 1.000001319 1.002337421 1.005743445);
    }
}


