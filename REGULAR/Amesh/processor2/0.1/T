FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.1";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(0.9999999997 0.9999999995 1.000000002 1.00000333 1.000000815);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary2to1
    {
        type            processor;
        value           uniform 0.9999999998;
    }
    procBoundary2to3
    {
        type            processor;
        value           nonuniform List<scalar> 3(0.999999988 0.999999988 1.000070641);
    }
    procBoundary2to8
    {
        type            processor;
        value           nonuniform List<scalar> 5(1.000000007 1.000000231 1.000000231 1.000509941 1.000858119);
    }
}


