FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1.6";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   nonuniform List<scalar> 5(0.9999999848 0.9999999831 0.9999999725 1.00002777 1.000032002);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            zeroGradient;
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary2to1
    {
        type            processor;
        value           uniform 1;
    }
    procBoundary2to3
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.00000012 1.00000012 1.014964713);
    }
    procBoundary2to8
    {
        type            processor;
        value           nonuniform List<scalar> 5(0.9999999238 1.000001327 1.000001327 1.002337104 1.005744754);
    }
}


