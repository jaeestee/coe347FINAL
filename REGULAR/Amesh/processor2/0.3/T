FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.3";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1.000000002 1.000000001 1.000000012 1.000009448 1.000008876);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary2to1
    {
        type            processor;
        value           uniform 0.9999999999;
    }
    procBoundary2to3
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.000000057 1.000000057 1.003546576);
    }
    procBoundary2to8
    {
        type            processor;
        value           nonuniform List<scalar> 5(1.000000002 1.000000284 1.000000284 1.000672915 1.00172486);
    }
}


