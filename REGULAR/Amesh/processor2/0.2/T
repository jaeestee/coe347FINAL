FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.2";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(0.9999999993 0.9999999987 1.000000028 1.00000875 1.000008101);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary2to1
    {
        type            processor;
        value           uniform 0.9999999989;
    }
    procBoundary2to3
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.00000002 1.00000002 1.001329289);
    }
    procBoundary2to8
    {
        type            processor;
        value           nonuniform List<scalar> 5(1.000000018 1.000000339 1.000000339 1.000661144 1.001847515);
    }
}


