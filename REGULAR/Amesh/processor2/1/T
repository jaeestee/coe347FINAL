FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(0.9999999955 0.9999999955 0.9999999915 1.000008096 1.000009105);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary2to1
    {
        type            processor;
        value           uniform 1;
    }
    procBoundary2to3
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.000000034 1.000000034 1.004275839);
    }
    procBoundary2to8
    {
        type            processor;
        value           nonuniform List<scalar> 5(0.999999977 1.00000038 1.00000038 1.000683812 1.001686045);
    }
}


