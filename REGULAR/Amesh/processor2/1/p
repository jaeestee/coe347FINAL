FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   nonuniform List<scalar> 5(0.9999999842 0.9999999842 0.9999999702 1.000027138 1.000032014);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            zeroGradient;
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary2to1
    {
        type            processor;
        value           uniform 1;
    }
    procBoundary2to3
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.000000118 1.000000118 1.015074496);
    }
    procBoundary2to8
    {
        type            processor;
        value           nonuniform List<scalar> 5(0.9999999199 1.000001363 1.000001363 1.002337007 1.005743508);
    }
}


