FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.8";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   nonuniform List<scalar> 5(1.000000124 1.001536451 1.021294264 1.015063436 1.041426608);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            zeroGradient;
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary3to2
    {
        type            processor;
        value           nonuniform List<scalar> 3(0.999999976 1.000031983 1.000031983);
    }
    procBoundary3to4
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.056606806 1.052568854);
    }
    procBoundary3to9
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.059413394 1.058144222);
    }
}


