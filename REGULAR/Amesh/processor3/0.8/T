FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.8";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1.000000036 1.000435145 1.006044368 1.004277366 1.011737446);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary3to2
    {
        type            processor;
        value           nonuniform List<scalar> 3(0.9999999931 1.000009111 1.000009111);
    }
    procBoundary3to4
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.015920726 1.014883736);
    }
    procBoundary3to9
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.017131042 1.016864051);
    }
}


