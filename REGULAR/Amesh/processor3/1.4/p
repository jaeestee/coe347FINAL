FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1.4";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   nonuniform List<scalar> 5(1.000000119 1.001511782 1.020779726 1.014958318 1.041890621);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            zeroGradient;
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary3to2
    {
        type            processor;
        value           nonuniform List<scalar> 3(0.9999999719 1.000031978 1.000031978);
    }
    procBoundary3to4
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.053926264 1.050567149);
    }
    procBoundary3to9
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.059472402 1.058407762);
    }
}


