FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "2.1";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   nonuniform List<scalar> 5(1.00000012 1.001515351 1.020780921 1.014965975 1.041853963);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            zeroGradient;
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary3to2
    {
        type            processor;
        value           nonuniform List<scalar> 3(0.9999999724 1.000031997 1.000031997);
    }
    procBoundary3to4
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.054005724 1.050679707);
    }
    procBoundary3to9
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.059467482 1.058389655);
    }
}


