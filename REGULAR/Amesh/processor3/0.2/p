FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.2";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   nonuniform List<scalar> 5(1.000000075 1.00018661 1.000868343 1.004909994 1.006877519);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            zeroGradient;
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary3to2
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.000000098 1.000028922 1.000028922);
    }
    procBoundary3to4
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.000778545 1.002141604);
    }
    procBoundary3to9
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.03185626 1.017037484);
    }
}


