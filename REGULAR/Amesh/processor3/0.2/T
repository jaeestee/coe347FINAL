FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.2";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1.00000002 1.000050369 1.000243611 1.001329289 1.00195318);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary3to2
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.000000028 1.000008101 1.000008101);
    }
    procBoundary3to4
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.000218563 1.000585789);
    }
    procBoundary3to9
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.009148425 1.004971497);
    }
}


