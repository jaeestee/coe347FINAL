FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.6";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1.00000006 1.000462153 1.006113482 1.00425004 1.012206509);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary3to2
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.000000005 1.000010014 1.000010014);
    }
    procBoundary3to4
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.014744754 1.013672548);
    }
    procBoundary3to9
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.016850457 1.016840138);
    }
}


