FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.6";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   nonuniform List<scalar> 5(1.000000208 1.001631568 1.02154404 1.014972669 1.043151652);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            zeroGradient;
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary3to2
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.000000016 1.000035222 1.000035222);
    }
    procBoundary3to4
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.052352809 1.048119861);
    }
    procBoundary3to9
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.05846546 1.058062568);
    }
}


