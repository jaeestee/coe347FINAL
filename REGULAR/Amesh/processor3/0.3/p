FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.3";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   nonuniform List<scalar> 5(1.000000201 1.001026354 1.006963045 1.012633261 1.025474113);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            zeroGradient;
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary3to2
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.000000042 1.000031839 1.000031839);
    }
    procBoundary3to4
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.008022821 1.014459354);
    }
    procBoundary3to9
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.052552664 1.042415691);
    }
}


