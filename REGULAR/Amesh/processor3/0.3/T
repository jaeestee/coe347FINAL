FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.3";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1.000000057 1.000286971 1.001972862 1.003546576 1.007286171);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary3to2
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.000000012 1.000008876 1.000008876);
    }
    procBoundary3to4
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.002282311 1.004136137);
    }
    procBoundary3to9
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.015140126 1.012384696);
    }
}


