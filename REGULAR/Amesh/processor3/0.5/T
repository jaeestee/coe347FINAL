FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.5";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1.000000063 1.000479642 1.00574567 1.004227788 1.012307199);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary3to2
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.000000011 1.000009913 1.000009913);
    }
    procBoundary3to4
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.013059334 1.011877452);
    }
    procBoundary3to9
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.016691086 1.016561046);
    }
}


