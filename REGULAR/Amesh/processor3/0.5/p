FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.5";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   nonuniform List<scalar> 5(1.000000221 1.001693646 1.020186527 1.014902909 1.043266686);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            zeroGradient;
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary3to2
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.000000037 1.000034971 1.000034971);
    }
    procBoundary3to4
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.046239318 1.041673484);
    }
    procBoundary3to9
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.057937329 1.057191577);
    }
}


