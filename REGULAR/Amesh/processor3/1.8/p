FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1.8";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   nonuniform List<scalar> 5(1.00000012 1.001515771 1.020785293 1.01496624 1.041861409);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            zeroGradient;
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary3to2
    {
        type            processor;
        value           nonuniform List<scalar> 3(0.9999999724 1.000031999 1.000031999);
    }
    procBoundary3to4
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.054004741 1.050666745);
    }
    procBoundary3to9
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.0594668 1.05839145);
    }
}


