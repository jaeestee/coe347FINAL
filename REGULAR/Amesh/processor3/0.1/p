FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.1";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   nonuniform List<scalar> 5(0.9999999582 1.000000951 1.000005556 1.000269369 1.00027512);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            zeroGradient;
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary3to2
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.000000007 1.000002689 1.000002689);
    }
    procBoundary3to4
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.000006461 1.00005232);
    }
    procBoundary3to9
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.006636628 1.001603889);
    }
}


