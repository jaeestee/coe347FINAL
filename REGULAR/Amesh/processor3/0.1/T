FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.1";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(0.999999988 1.000000264 1.000001601 1.000070641 1.000078075);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary3to2
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.000000002 1.000000815 1.000000815);
    }
    procBoundary3to4
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.00000185 1.00001496);
    }
    procBoundary3to9
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.001752178 1.000439834);
    }
}


