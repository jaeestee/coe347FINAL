FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1.7";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   nonuniform List<scalar> 5(1.00000012 1.001515882 1.020790332 1.014966184 1.0418647);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            zeroGradient;
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary3to2
    {
        type            processor;
        value           nonuniform List<scalar> 3(0.9999999725 1.000032002 1.000032002);
    }
    procBoundary3to4
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.0539909 1.050656322);
    }
    procBoundary3to9
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.059467824 1.058390181);
    }
}


