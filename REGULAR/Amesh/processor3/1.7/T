FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1.7";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1.000000035 1.000428906 1.005910748 1.004245896 1.01188488);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary3to2
    {
        type            processor;
        value           nonuniform List<scalar> 3(0.9999999921 1.000009108 1.000009108);
    }
    procBoundary3to4
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.015205781 1.014365921);
    }
    procBoundary3to9
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.017143947 1.016924928);
    }
}


