FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.7";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1.000000051 1.000439124 1.006245051 1.004263553 1.011944369);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary3to2
    {
        type            processor;
        value           nonuniform List<scalar> 3(0.9999999935 1.000009828 1.000009828);
    }
    procBoundary3to4
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.015537338 1.014377595);
    }
    procBoundary3to9
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.017043216 1.016858869);
    }
}


