FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1.000000034 1.000426834 1.005882804 1.004275839 1.011736881);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary3to2
    {
        type            processor;
        value           nonuniform List<scalar> 3(0.9999999915 1.000009105 1.000009105);
    }
    procBoundary3to4
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.015476292 1.014602908);
    }
    procBoundary3to9
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.017196103 1.016907454);
    }
}


