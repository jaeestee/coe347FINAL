FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "3.6";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   nonuniform List<scalar> 5(1.00000012 1.001515256 1.020781763 1.014965593 1.041855384);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            zeroGradient;
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary3to2
    {
        type            processor;
        value           nonuniform List<scalar> 3(0.9999999724 1.000031997 1.000031997);
    }
    procBoundary3to4
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.053999749 1.050675569);
    }
    procBoundary3to9
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.059467456 1.058390295);
    }
}


