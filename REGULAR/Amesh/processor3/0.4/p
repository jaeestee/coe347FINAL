FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.4";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   nonuniform List<scalar> 5(1.000000206 1.001671591 1.016092194 1.014942673 1.039148404);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            zeroGradient;
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary3to2
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.000000034 1.000034293 1.000034293);
    }
    procBoundary3to4
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.027989231 1.031647989);
    }
    procBoundary3to9
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.056995836 1.053963112);
    }
}


