FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.4";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1.000000059 1.000472482 1.004575339 1.004235476 1.011171983);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary3to2
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.00000001 1.000009606 1.000009606);
    }
    procBoundary3to4
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.007954238 1.009054163);
    }
    procBoundary3to9
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.016426474 1.015658923);
    }
}


