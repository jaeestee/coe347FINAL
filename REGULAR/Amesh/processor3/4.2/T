FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "4.2";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1.000000035 1.000428718 1.005908459 1.004245658 1.011882755);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary3to2
    {
        type            processor;
        value           nonuniform List<scalar> 3(0.9999999921 1.000009107 1.000009107);
    }
    procBoundary3to4
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.015208877 1.014370164);
    }
    procBoundary3to9
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.017143572 1.016925564);
    }
}


