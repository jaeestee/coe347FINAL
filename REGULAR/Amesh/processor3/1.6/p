FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1.6";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   nonuniform List<scalar> 5(1.00000012 1.001515521 1.020797852 1.014964713 1.041873062);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            zeroGradient;
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary3to2
    {
        type            processor;
        value           nonuniform List<scalar> 3(0.9999999725 1.000032002 1.000032002);
    }
    procBoundary3to4
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.053962425 1.050638102);
    }
    procBoundary3to9
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.059468445 1.058390741);
    }
}


