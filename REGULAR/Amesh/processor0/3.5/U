dimensions      [0 1 -1 0 0 0 0];

internalField   nonuniform List<vector> 5((3 -5.925344375e-16 0) (3 -1.252710547e-15 0) (3 -1.547513709e-15 0) (3 -5.049612452e-16 0) (3 -1.648237329e-15 0));

