FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0.5";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   nonuniform List<vector> 5((3 5.149110234e-17 0) (3 -7.063107632e-16 0) (3 -1.125978824e-15 0) (3 -4.029520109e-16 0) (3 -8.062342316e-16 0));

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (3 0 0);
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            slip;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform (3 -1.726211421e-15 0);
    }
    procBoundary0to6
    {
        type            processor;
        value           nonuniform List<vector> 4((3 -1.660509752e-15 0) (3 9.037871886e-17 0) (3 -1.660509752e-15 0) (3 5.607536352e-16 0));
    }
}


