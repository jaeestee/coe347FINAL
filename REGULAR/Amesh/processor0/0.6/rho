FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.6";
    object      rho;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   nonuniform List<scalar> 5(1.40000504 1.40000504 1.40000504 1.40000504 1.40000504);

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform 1.40000504;
    }
    outlet
    {
        type            calculated;
        value           nonuniform 0();
    }
    bottom
    {
        type            calculated;
        value           nonuniform List<scalar> 3(1.40000504 1.40000504 1.40000504);
    }
    top
    {
        type            calculated;
        value           nonuniform 0();
    }
    obstacle
    {
        type            calculated;
        value           nonuniform 0();
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform 1.40000504;
    }
    procBoundary0to6
    {
        type            processor;
        value           nonuniform List<scalar> 4(1.40000504 1.40000504 1.40000504 1.40000504);
    }
}


