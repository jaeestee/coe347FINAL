FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "1.4";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   nonuniform List<vector> 5((3 5.910679374e-16 0) (3 -1.283533525e-15 0) (3 -1.77560375e-15 0) (3 3.750347914e-16 0) (3 5.010878716e-17 0));

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (3 0 0);
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            slip;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform (3 -1.156554648e-15 0);
    }
    procBoundary0to6
    {
        type            processor;
        value           nonuniform List<vector> 4((3 -8.520674063e-16 0) (3 5.251241108e-17 0) (3 -8.520674063e-16 0) (3 2.330717706e-15 0));
    }
}


