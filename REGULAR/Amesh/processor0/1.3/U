dimensions      [0 1 -1 0 0 0 0];

internalField   nonuniform List<vector> 5((3 5.325042782e-16 0) (3 -2.59560017e-17 0) (3 -4.9953244e-16 0) (3 -4.107541853e-17 0) (3 -1.224998925e-16 0));

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (3 0 0);
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            slip;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform (3 -1.16428478e-15 0);
    }
    procBoundary0to6
    {
        type            processor;
        value           nonuniform List<vector> 4((3 1.253197486e-16 0) (3 1.850512187e-16 0) (3 1.253197486e-16 0) (3 1.80980937e-15 0));
    }
}


