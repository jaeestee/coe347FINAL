dimensions      [0 1 -1 0 0 0 0];

internalField   nonuniform List<vector> 5((3 7.020306601e-16 0) (3 -1.278910078e-15 0) (3 -1.931259792e-15 0) (3 -1.55120439e-16 0) (3 -4.553880604e-16 0));

boundaryField
{
