FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "1.8";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   nonuniform List<vector> 5((3 5.783040914e-17 0) (3 1.038977906e-15 0) (3 9.997449206e-17 0) (3 4.368777238e-17 0) (3 1.561975883e-15 0));

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (3 0 0);
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            slip;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform (3 -1.15715603e-16 0);
    }
    procBoundary0to6
    {
        type            processor;
        value           nonuniform List<vector> 4((3 6.996541034e-16 0) (3 -3.296925225e-16 0) (3 6.996541034e-16 0) (3 -1.181679364e-16 0));
    }
}


