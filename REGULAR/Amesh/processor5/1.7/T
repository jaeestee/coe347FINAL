FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1.7";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1.020079399 1.016088899 1.012932471 1.007139153 1.001713533);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 1;
        value           nonuniform List<scalar> 2(1.016088669 1.007139062);
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary5to4
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.015205781 1.014365921 1.010382575);
    }
    procBoundary5to11
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.002710849 1.002710849 0.9936934216);
    }
}


