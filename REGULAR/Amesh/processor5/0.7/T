FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.7";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1.016991153 1.010872688 1.011775787 1.002133466 1.001218773);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 1;
        value           nonuniform List<scalar> 2(1.01083851 1.002096802);
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary5to4
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.015537338 1.014377595 1.010976147);
    }
    procBoundary5to11
    {
        type            processor;
        value           nonuniform List<scalar> 3(0.997872065 0.997872065 0.9920764601);
    }
}


