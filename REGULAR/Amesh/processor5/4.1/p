FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "4.1";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   nonuniform List<scalar> 5(1.072012103 1.0577282 1.045084294 1.023474882 0.9985153035);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            zeroGradient;
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary5to4
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.053999745 1.050675585 1.033385824);
    }
    procBoundary5to11
    {
        type            processor;
        value           nonuniform List<scalar> 3(0.9996505007 0.9996505007 0.9582531387);
    }
}


