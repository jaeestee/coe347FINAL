FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.6";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   nonuniform List<scalar> 5(1.053573497 1.027323938 1.033599874 0.9962643975 0.9894687372);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            zeroGradient;
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary5to4
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.052352809 1.048119861 1.031678746);
    }
    procBoundary5to11
    {
        type            processor;
        value           nonuniform List<scalar> 3(0.9757453067 0.9757453067 0.9480636454);
    }
}


