FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "2";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1.02009942 1.016116461 1.012938232 1.007152383 1.001704625);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 1;
        value           nonuniform List<scalar> 2(1.016116416 1.007152355);
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary5to4
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.015211123 1.014370929 1.010394932);
    }
    procBoundary5to11
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.002706576 1.002706576 0.993674213);
    }
}


