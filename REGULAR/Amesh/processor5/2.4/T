FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "2.4";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1.020101498 1.016122727 1.012938208 1.007160382 1.001708154);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 1;
        value           nonuniform List<scalar> 2(1.016122724 1.007160387);
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary5to4
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.015209198 1.014370544 1.010395197);
    }
    procBoundary5to11
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.002717969 1.002717969 0.9936837015);
    }
}


