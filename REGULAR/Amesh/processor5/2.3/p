FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "2.3";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   nonuniform List<scalar> 5(1.072017542 1.057729875 1.04509044 1.023474076 0.9985162297);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            zeroGradient;
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary5to4
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.05400198 1.050677907 1.033393398);
    }
    procBoundary5to11
    {
        type            processor;
        value           nonuniform List<scalar> 3(0.9996431481 0.9996431481 0.9582455406);
    }
}


