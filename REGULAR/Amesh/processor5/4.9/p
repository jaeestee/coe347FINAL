FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "4.9";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   nonuniform List<scalar> 5(1.072012116 1.057728202 1.045084355 1.023474925 0.9985153083);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            zeroGradient;
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary5to4
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.053999749 1.050675581 1.033385487);
    }
    procBoundary5to11
    {
        type            processor;
        value           nonuniform List<scalar> 3(0.9996505693 0.9996505693 0.958253106);
    }
}


