FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "2.5";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1.020101129 1.016122629 1.012937761 1.00716048 1.0017081);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 1;
        value           nonuniform List<scalar> 2(1.016122632 1.007160473);
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary5to4
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.015209004 1.014370373 1.010394585);
    }
    procBoundary5to11
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.002718861 1.002718861 0.9936841525);
    }
}


