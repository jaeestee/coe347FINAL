FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "4.2";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1.020100261 1.016121725 1.012937181 1.007160002 1.001707995);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 1;
        value           nonuniform List<scalar> 2(1.016121725 1.007159998);
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary5to4
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.015208877 1.014370164 1.01039401);
    }
    procBoundary5to11
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.002718947 1.002718947 0.9936844927);
    }
}


