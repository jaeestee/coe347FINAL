FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1.1";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   nonuniform List<scalar> 5(1.073031441 1.058375136 1.046432098 1.023799441 1.000420749);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            zeroGradient;
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary5to4
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.054477098 1.050947051 1.032698);
    }
    procBoundary5to11
    {
        type            processor;
        value           nonuniform List<scalar> 3(0.999023051 0.999023051 0.9594223592);
    }
}


