    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary11to4
    {
        type            processor;
        value           nonuniform List<scalar> 2(0.9618993922 0.950107812);
