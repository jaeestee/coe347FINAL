FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1.8";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1.002705157 0.9936779013 1.000044038 1.006269092 1.01269869);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 1;
        value           nonuniform List<scalar> 3(1.00270524 1.000043402 1.012700666);
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary11to4
    {
        type            processor;
        value           nonuniform List<scalar> 2(0.9952697795 0.9991711533);
    }
    procBoundary11to5
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.007145694 1.001705857 1.001705857);
    }
    procBoundary11to17
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.028058811 1.022811609);
    }
