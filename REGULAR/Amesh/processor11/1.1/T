    {
        type            inletOutlet;
        inletValue      uniform 1;
        value           nonuniform List<scalar> 3(1.002581013 0.9997474169 1.012833632);
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary11to4
    {
        type            processor;
        value           nonuniform List<scalar> 2(0.9957236683 0.9988407342);
    }
    procBoundary11to5
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.007194691 1.00171618 1.00171618);
