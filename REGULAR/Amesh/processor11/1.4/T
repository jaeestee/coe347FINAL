FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1.4";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 5(1.002803918 0.9936954825 1.000097421 1.006224835 1.012860079);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 1;
        value           nonuniform List<scalar> 3(1.002804764 1.000097338 1.012862775);
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary11to4
    {
        type            processor;
        value           nonuniform List<scalar> 2(0.9954141611 0.9990999096);
    }
    procBoundary11to5
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.007223068 1.001714732 1.001714732);
    }
    procBoundary11to17
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.028060934 1.023054534);
    }
