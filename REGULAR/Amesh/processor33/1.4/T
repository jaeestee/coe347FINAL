    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary33to26
    {
        type            processor;
        value           uniform 1.02719613;
    }
    procBoundary33to27
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.052648143 0.9933624661);
