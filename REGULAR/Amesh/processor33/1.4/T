dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 8(1.02799864 1.017256174 1.017146644 1.016926055 1.004244477 1.011893565 1.00042786 1.005909727);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
