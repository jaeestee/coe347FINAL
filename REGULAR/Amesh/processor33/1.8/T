dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 8(1.02799187 1.017280471 1.017144697 1.016926035 1.004246521 1.011886785 1.000428991 1.005912068);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary33to26
    {
        type            processor;
        value           uniform 1.027199653;
    }
    procBoundary33to27
    {
        type            processor;
        value           nonuniform List<scalar> 2(1.052648867 0.9933688611);
    }
    procBoundary33to32
    {
        type            processor;
        value           nonuniform List<scalar> 3(1.001686534 1.000009107 1.000000035);
