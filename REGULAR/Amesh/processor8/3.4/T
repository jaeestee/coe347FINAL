    {
        type            inletOutlet;
        inletValue      nonuniform 0();
        value           nonuniform 0();
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary8to1
    {
        type            processor;
        value           uniform 1.000000014;
    }
    procBoundary8to2
    {
        type            processor;
        value           nonuniform List<scalar> 5(0.9999999956 0.9999999952 1.000008289 1.000008289 1.000009107);
    }
    procBoundary8to9
    {
        type            processor;
        value           uniform 1.017143513;
