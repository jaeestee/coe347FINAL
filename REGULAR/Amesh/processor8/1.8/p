    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    obstacle
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary8to1
    {
        type            processor;
        value           uniform 1.000000047;
    }
    procBoundary8to2
    {
        type            processor;
        value           nonuniform List<scalar> 5(0.9999999847 0.9999999831 1.000027753 1.000027753 1.000031999);
